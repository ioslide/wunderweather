<view class="container">
    <view class="top">
        <image class="homeLogo" src="../../images/homeLogo.png"></image>
        <view class="title">微信对话开放平台</view>
        <view class="plugin">小程序</view>
    </view>
    <view class="empty"></view>
    <view class="bottom">
        <view class="bottom_UI">
            <view class="UI">有UI语音助手</view>
            <view class="UI_info">有UI，直接使用标记组件，无需接口调用</view>
