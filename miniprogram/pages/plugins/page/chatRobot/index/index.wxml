        <view class="bottom_noUI">
            <view class="noUI">无UI语音助手</view>
            <view class="noUI_info">无UI，调用Api即可使用</view>
            <view bindtap="bindtapOpenNoUI" class="noUI_go">进入</view>
        </view>
    </view>
</view>
