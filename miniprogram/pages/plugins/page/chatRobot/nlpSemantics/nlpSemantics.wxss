.container {
    width: 100vw;
    height: 100vh;
    background: rgba(247,251,252,1);
}

.nav-wrap {
    background-image: url(https://res.wx.qq.com/mmspraiweb_node/dist/static/nlpagain/image/banner22.png);
    background-size: 100% 100%;
}

.content {
    position: relative;
    top: -47.5px;
    left: 0;
    width: 100%;
    height: 100%;
}

.words {
    font-size: 18px;
    font-family: PingFangSC-Semibold,PingFang SC;
    font-weight: 600;
    color: rgba(255,255,255,1);
}

.content_card {
    margin-top: 15px;
}

.content_card:nth-child(1) {
    margin-top: 0;
}

.content_card_all {
    margin: 0 15.5px 0 18.5px;
    height: 95px;
    background: rgba(255,255,255,1);
    box-shadow: 0px 1.5px 6px 3.5px rgba(70,84,104,0.09);
    border-radius: 50px 11px 11px 50px;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    padding-left: 16.5px;
}

.content_card_all_circle {
    width: 64px;
    height: 64px;
    background: rgba(245,249,255,1);
    border-radius: 50%;
    margin-right: 19px;
    display: flex;
    justify-content: center;
    align-items: center;
}

.content_card_all_words {
    width: 232px;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}

.content_card_all_words_title {
    font-size: 18px;
    font-family: PingFangSC-Semibold,PingFang SC;
    font-weight: 600;
    color: rgba(48,49,51,1);
}

.content_card_all_words_paras {
    margin-top: 3px;
    font-size: 12px;
    font-family: PingFangSC-Regular,PingFang SC;
    font-weight: 400;
    color: rgba(96,98,102,1);
    width: 100%;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.content_card_all_circle_img {
    width: 20px;
    height: 20px;
}