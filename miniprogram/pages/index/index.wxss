.bird .body {
  width: 0.6em;
  height: 0.3em;
  background-color: rgb(0, 0, 0);
  border-radius: 50%;
  position: relative;
  box-shadow: 0 0.3em 0 0.03em rgba(0, 0, 0, 0.75), 0 0.05em 0 0.05em rgba(239, 255, 153, 0.2);
}

.bird .wing {
  width: 1em;
  height: 0.185em;
  background-color: rgb(0, 0, 0);
  position: relative;
}

.bird .wing.left {
  transform-origin: right top;
  border-radius: 0 20% 20% 0;
  transform: rotate(20deg);
}

.bird .wing.left::before {
  content: "";
  display: block;
  position: absolute;
  width: 1em;
  height: 0.185em;
  background-color: rgb(0, 0, 0);
  left: -1em;
  transform-origin: right top;
  transform: rotate(12.5deg);
  border-radius: 50% 0 0 1em / 0.1em;
}

.bird .wing.right {
  transform-origin: left top;
  transform: rotate(-20deg);
  border-radius: 20% 0 0 20%;
}

.bird .wing.right::before {
  content: "";
  display: block;
  position: absolute;
  width: 1em;
  height: 0.185em;
  background-color: rgb(0, 0, 0);
  right: -1em;
  transform-origin: left;
  transform: rotate(-12.5deg);
  border-radius: 0 50% 1em 0 / 0.1em;
}

@keyframes up-down {
  0% {
    transform: translateY(0em);
  }

  55% {
    transform: translateY(-0.7em);
  }

  100% {
    transform: translateY(0em);
  }
}

@keyframes main-flap-left {
  0% {
    transform: rotate(20deg);
  }

  45% {
    transform: rotate(-20deg);
  }

  100% {
    transform: rotate(20deg);
  }
}

@keyframes main-flap-right {
  0% {
    transform: rotate(-20deg);
  }

  45% {
    transform: rotate(20deg);
  }

  100% {
    transform: rotate(-20deg);
  }
}

@keyframes minor-flap-right {
  0% {
    transform: rotate(-12.5deg);
  }

  45% {
    transform: rotate(25deg);
  }

  100% {
    transform: rotate(-12.5deg);
  }
}

@keyframes minor-flap-left {
  0% {
    transform: rotate(12.5deg);
  }

  45% {
    transform: rotate(-25deg);
  }

  100% {
    transform: rotate(12.5deg);
  }
}

