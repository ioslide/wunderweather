
.sunSetBox {
  width: 210px;
  overflow: hidden;
  height: 115px;
  position: relative;
  display: flex;
  padding-top: 20px;
  padding-left: 10px;
  padding-right: 10px;
  
}
.sunSetBg {
  width: 190px;
  height: 95px;
  margin: 0 auto;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  background-repeat: no-repeat;
  background-position: 50%;
  position: absolute;
  display: flex;  
}

.sunShadowBox {
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: relative;
}

.lightSunRiseShadow {
  width: 100%;
  height: 100%;
  border: 1px dashed rgb(193, 198, 204);
  border-radius: 0 0 50% 50%/0 0 100% 100% ;
  border-top: none;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  display: inline-block;
  background: radial-gradient(circle at 0% -70%, rgb(232, 235, 238) 0%, rgb(224, 229, 233) 85%);
  z-index:3;
  left: 0;
  top: 0;
}
.darkSunRiseShadow {
  width: 190px;
  height: 95px;
  border: 1px dashed rgb(255,201,84);
  border-radius: 0 0 50% 50%/0 0 100% 100% ;
  border-top: none;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  position: relative;
  margin: 0 auto;
  /* overflow: hidden; */
  display: inline-block;
  background: linear-gradient(to bottom, rgba(168, 143, 93, 0.262) 40%, rgba(175, 149, 97, 0.262) 100%);
  z-index:3;
  left: 0;
  bottom: 0;
}
.lightSunShadow {
  width: 100%;
  height: 200%;
  display: inline-block;
  border-radius: 50%;
  position: relative;
  overflow: hidden;
  border: 1px dashed rgb(224, 229, 233);
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  z-index:2
}
.darkSunShadow {
  width: 100%;
  height: 200%;
  display: inline-block;
  border-radius: 50%;
  position: relative;
  overflow: hidden;
  border: 1px dashed rgb(92,92,92);
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
  z-index:2
}
.lightFullDayBorder{
  border: 1px dashed rgb(224, 229, 233);
}
.lightSunriseBorder{
  border: 1px dashed rgb(193, 198, 204);
}
.darkSunriseBorder{
