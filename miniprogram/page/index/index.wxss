.item-text-dark {
  margin-top: 8px;
  font-size: 13px;
  font-weight: 200;
  color: #FAFAFA
}

.item-text-light {
  margin-top: 8px;
  font-size: 13px;
  font-weight: 200;
  color: #8799a3
}

.soft-button-light {
  border-radius: 20px;
  border: none;
  background: #f5f5f5;
  box-shadow: -9px -9px 9px 0 rgba(255, 255, 255, 0.62),
    -1px -1px 1px 0px rgba(255, 255, 255, 0.3),
    1px 1px 1px 0px rgba(206, 206, 206, 0.15),
    9px 9px 9px 0 rgba(206, 206, 206, 0.3);
  /* margin: 14px; */
  padding: 12px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
}

.soft-button-dark {
  border-radius: 20px;
  border: none;
  background: #4B4B4B;
  padding: 12px;
  -webkit-border-radius: 20px;
  -moz-border-radius: 20px;
  -ms-border-radius: 20px;
  -o-border-radius: 20px;
}

.soft-button-pushed-only {
  animation: fadeIn 0.3s linear;
  box-shadow: inset 9px 9px 9px 0 rgba(206, 206, 206, 0.3),
    inset -9px -9px 9px 0 rgba(255, 255, 255, 0.6);
}

.soft-button-pushed,
soft-button-light:active,
soft-button-light:hover {
  /* animation: fade-in 0.1s linear; */
  box-shadow: inset 9px 9px 9px 0 rgba(206, 206, 206, 0.3),
    inset -9px -9px 9px 0 rgba(255, 255, 255, 0.6),
    -9px -9px 9px 0 rgba(255, 255, 255, 0.62),
    -1px -1px 1px 0px rgba(255, 255, 255, 0.3),
    1px 1px 1px 0px rgba(206, 206, 206, 0.15),
    9px 9px 9px 0 rgba(206, 206, 206, 0.3);
}

.soft-button-convex {
  animation: fadeIn 0.3s linear;
  box-shadow: inset -9px -9px 9px 0 rgba(206, 206, 206, 0.3),
    inset 9px 9px 9px 0 rgba(255, 255, 255, 0.6),
    -9px -9px 9px 0 rgba(255, 255, 255, 0.62),
    -1px -1px 1px 0px rgba(255, 255, 255, 0.3),
    1px 1px 1px 0px rgba(206, 206, 206, 0.15),
    9px 9px 9px 0 rgba(206, 206, 206, 0.3);
}

.item-bg-active {
  animation: fadeIn 1s linear;
  background: #7093BF;
  /* background:#4197FE */
  border-radius: 50%;
  width: 50px;
  height: 50px;
}

.item-bg-grey {
  animation: fade-in 0.6s ease-in;
  background: #4B4B4B;
  border-radius: 50%;
  width: 50px;
  height: 50px;
}

.shareFriends {
  border: none;
  background: none;
  width: 33.33%;
  height: 100%;
  position: absolute;
  bottom: 1px
}

.headImageIcon-RightBottom {
  position: absolute;
  right: 0;
  bottom: 0;
}

.headImageIcon-Center {
  justify-content: center;
  align-items: center;
  position: absolute;
}

.dailyChart {
  width: 100%;
  height: 260px;
}

.rainChart {
  width: 100%;
  height: 200px;
}
