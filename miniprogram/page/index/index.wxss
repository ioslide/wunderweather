.bird .body {
  width: 0.6em;
  height: 0.3em;
  background-color: rgb(0, 0, 0);
  border-radius: 50%;
  position: relative;
  box-shadow: 0 0.3em 0 0.03em rgba(0, 0, 0, 0.75), 0 0.05em 0 0.05em rgba(239, 255, 153, 0.2);
}

.bird .wing {
  width: 1em;
  height: 0.185em;
  background-color: rgb(0, 0, 0);
  position: relative;
}

.bird .wing.left {
  transform-origin: right top;
  border-radius: 0 20% 20% 0;
  transform: rotate(20deg);
}

.bird .wing.left::before {
  content: "";
  display: block;
  position: absolute;
  width: 1em;
  height: 0.185em;
  background-color: rgb(0, 0, 0);
  left: -1em;
  transform-origin: right top;
  transform: rotate(12.5deg);
  border-radius: 50% 0 0 1em / 0.1em;
}

.bird .wing.right {
  transform-origin: left top;
  transform: rotate(-20deg);
  border-radius: 20% 0 0 20%;
}

.bird .wing.right::before {
  content: "";
  display: block;
  position: absolute;
  width: 1em;
  height: 0.185em;
  background-color: rgb(0, 0, 0);
  right: -1em;
  transform-origin: left;
  transform: rotate(-12.5deg);
  border-radius: 0 50% 1em 0 / 0.1em;
}

@keyframes up-down {
  0% {
    transform: translateY(0em);
  }

  55% {
    transform: translateY(-0.7em);
  }

  100% {
    transform: translateY(0em);
  }
}

@keyframes main-flap-left {
  0% {
    transform: rotate(20deg);
  }

  45% {
    transform: rotate(-20deg);
  }

  100% {
    transform: rotate(20deg);
  }
}

@keyframes main-flap-right {
  0% {
    transform: rotate(-20deg);
  }

  45% {
    transform: rotate(20deg);
  }

  100% {
    transform: rotate(-20deg);
  }
}

@keyframes minor-flap-right {
  0% {
    transform: rotate(-12.5deg);
  }

  45% {
    transform: rotate(25deg);
  }

  100% {
    transform: rotate(-12.5deg);
  }
}

@keyframes minor-flap-left {
  0% {
    transform: rotate(12.5deg);
  }

  45% {
    transform: rotate(-25deg);
  }

  100% {
    transform: rotate(12.5deg);
  }
}

.wind-mills {
  width: 60%;
  height: 2rem;
  position: absolute;
  right: 1.2rem;
  bottom: 1.5rem;
}

.wind-mills .wind-mill {
  width: 100%;
  height: 100%;
  position: absolute;
}

.wind-mills .wind-mill-1 {
  left: 5%;
  bottom: 0;
  transform: scale(1);
}

.wind-mills .wind-mill-2 {
  left: 24%;
  bottom: -2%;
  transform-origin: center;
  transform: scale(0.8) rotateY(45deg);
  opacity: 0.85;
}

.wind-mills .wind-mill-3 {
  left: 50%;
  bottom: -10%;
  transform-origin: center;
  transform: scale(1) rotateY(-25deg);
}

.wind-mills .wind-mill-4 {
  left: 70%;
  bottom: -20%;
  transform-origin: center;
  transform: scale(0.5) rotateY(5deg);
  opacity: 0.8;
}

.wind-mills .wind-mill .foot-dark {
  width: 0.1em;
  height: 1em;
  background-color: rgb(255, 0, 0);
  box-sizing: border-box;
  background-clip: padding-box;
  border-top: 0px;
  border-right: 0.03em solid transparent;
  border-bottom: 1em solid #16110e;
  border-left: 0.1em solid rgba(255, 255, 255, 0.7);
  position: absolute;
  bottom: 0;
}

.wind-mills .wind-mill .foot-light {
  width: 0.1em;
  height: 1em;
  background-color: rgb(255, 0, 0);
  box-sizing: border-box;
  background-clip: padding-box;
  border-top: 0px;
  border-right: 0.03em solid transparent;
  border-bottom: 1em solid rgba(135, 153, 163, 0.342);
  border-left: 0.1em solid rgba(135, 153, 163, 0.842);
  position: absolute;
  bottom: 0;
}

.wind-mills .wind-mill .foot-light::before {
  content: "";
  display: block;
  width: 0.2em;
  height: 0.075em;
  background-color: rgb(255, 255, 255);
  position: absolute;
  top: -0.05em;
  border-radius: 50%;
}

.wind-mills .wind-mill .foot-light::after {
  content: "";
  display: block;
  width: 20em;
  height: 0.07em;
  background-color: rgba(224, 201, 201, 0.301);
  position: absolute;
  bottom: -1em;
  transform-origin: left;
  transform: rotate(7deg);
}

.wind-mills .wind-mill .foot-dark::before {
  content: "";
  display: block;
  width: 0.2em;
  height: 0.075em;
  background-color: #ffffff;
  position: absolute;
  top: -0.05em;
  border-radius: 50%;
}

.wind-mills .wind-mill .foot-dark::after {
  content: "";
  display: block;
  width: 20em;
  height: 0.07em;
  background-color: rgba(224, 201, 201, 0.301);
  position: absolute;
  bottom: -1em;
  transform-origin: left;
  transform: rotate(7deg);
}

.wind-mills .wind-mill .wings {
  width: 1.2rem;
  height: 1.2rem;
  border-radius: 50%;
  position: absolute;
  bottom: 0.3rem;
  left: -0.6rem;
}

.wind-mills .wind-mill .wings .wing {
  width: 0.1rem;
  left: 49%;
  bottom: 50%;
  position: absolute;
  box-sizing: border-box;
  border-right: 0px;
  border-left: 0.08rem solid #ffffff;
  border-top: 0.6rem solid transparent;
  border-bottom: 0.08rem solid transparent;
}

.wind-mills .wind-mill .wings .wing::before,
.wind-mills .wind-mill .wings .wing::after {
  content: "";
  display: block;
  position: absolute;
  border-right: 0;
  border-left: 0.08rem solid #ffffff;
  border-top: 0.6rem solid transparent;
  border-bottom: 0.08rem solid transparent;
  bottom: -0.08rem;
  left: -0.08rem;
  transform-origin: bottom left;
}

.wind-mills .wind-mill .wings .wing::before {
  transform: rotate(120deg);
}

.wind-mills .wind-mill .wings .wing::after {
  transform: rotate(240deg);
}

