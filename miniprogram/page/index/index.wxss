  height: 9rem;
  position: relative;
  /* box-shadow: 0.2rem 0.2rem 0.5rem 0 rgba(0, 0, 0, 0.1); */
  overflow: hidden;
}

.windcontainer-dark::before {
  content: "";
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  box-shadow: inset 0.25rem 0.25rem 1rem 0 rgba(8, 8, 8, 0.1);
  z-index: 10000;
}

.windcontainer-light {
