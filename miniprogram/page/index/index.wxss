.forecast-list {
  display: flex;
  padding-top: 30rpx;
  padding-bottom: 50rpx;
}

.forecast-item-dark {
  font-family: 'Lato', sans-serif;
  display: flex;
  flex-shrink: 0;
  flex-direction: column;
  align-items: center;
  margin: 5px 1px 5px 1px;
  border-radius: 6px;
}

.forecast-item-dark:first-child {
  background: #131313;
  margin: 5px 2px 5px 0px;
}

.forecast-item-light {
  font-family: 'Lato', sans-serif;
  display: flex;
  flex-shrink: 0;
  flex-direction: column;
  align-items: center;
  margin: 5px 1px 5px 1px;
  border-radius: 6px;
}

.forecast-item-light:first-child {
  border-radius: 20px;
  border: none;
  box-shadow: -9px -9px 9px 0 rgba(255, 255, 255, 0.62),
    -1px -1px 1px 0px rgba(255, 255, 255, 0.3),
    1px 1px 1px 0px rgba(206, 206, 206, 0.15),
    9px 9px 9px 0 rgba(206, 206, 206, 0.3);
  background: #f5f5f5;
  margin: 5px 2px 5px 0px;
}

.forecast-time-light {
  font-size: 30rpx;
  line-height: 42rpx;
  padding-top: 10px;
  padding-bottom: 10px;
  font-weight: 200;
  color: #4a5568
}

.forecast-time-dark {
  font-size: 30rpx;
  line-height: 42rpx;
  padding-top: 10px;
  padding-bottom: 10px;
  color: #ffffff
}

.forecast-weather {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  width: 50rpx;
  height: 50rpx;
}

.forecast-temp {
  font-size: 30rpx;
  line-height: 50rpx;
  padding-top: 10px;
  padding-bottom: 10px;
}

