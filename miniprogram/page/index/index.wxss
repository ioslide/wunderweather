@keyframes fade-in {
  0% {
    opacity: 0;
  }

  100% {
    opacity: 1;
  }
}

@keyframes fade-out {
  0% {
    opacity: 1;
  }

  100% {
    opacity: 0;
  }
}

page {
  overflow: visible;
  background: #F5F6F7
}

.dark-page {
  animation: fade-in 0.6s ease-in;
  background: #010101;
  overflow: visible;
}

.light-page {
  animation: fade-in 0.6s ease-in;
  background: #F5F6F7;
  overflow: visible;
}

.page-hidden {
  overflow: hidden
}

button::after {
  border: none;
}

.indexFixed {
  position: fixed;
}

.weatherImg {
  border-radius: 6px;
  width: 100%;
  height: 300px;
  background-size: cover;
  background-position: 50% 99%;
}

.xhyicon {
  padding: 42px 0;
  opacity: 0;
  display: flex;
  box-shadow: 0 30rpx 20rpx rgba(0, 0, 0, 0.2);
  overflow: hidden;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0rpx;
  background-color: rgba(255, 255, 255, 0);
  z-index: 999;
}

.official-account {
  display: flex;
  box-shadow: 0 30rpx 20rpx rgba(0, 0, 0, 0.2);
  overflow: hidden;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0rpx;
  padding: 0;
  background-color: rgba(255, 255, 255, 0);
  z-index: 999;
}

.buttoncontainer {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 0vh;
}

.buttoncontainer a {
  font-size: 1rem;
  padding: 0.1rem 1.5rem;
  color: #4b86bd;
  text-transform: uppercase;
}

.buttoncontainer .btn {
  text-decoration: none;
  border: 1px solid rgba(146, 148, 248, 0);
  position: relative;
  border-radius: 50px;
  overflow: hidden;
}

.buttoncontainer .btn:hover {
  box-shadow: 1px 1px 25px 10px rgba(146, 148, 248, 0.4);
}

.buttoncontainer .btn:before {
  content: "";
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(120deg, transparent, rgba(146, 148, 248, 0.4), transparent);
  transition: all 650ms;
}

.buttoncontainer .btn:hover:before {
  left: 100%;
}

.poetry {
  height: 100%;
  width: 30px;
  text-align: center;
  font-size: 22px;
  letter-spacing: 2px;
  writing-mode: vertical-lr;
}

.white {
  fill: #fff;
}

.gray {
  fill: #e0e0e0;
}

.yellow {
  fill: #ffeb3b;
}

.slideWeatherImg {
  border-radius: 2px;
  width: 98%;
  background-size: cover;
  padding: 20px 0;
  background-position: 50% 99%;
  height: 170px;
}

.share {
  display: flex;
  flex-direction: row;
  justify-content: center !important;
  align-items: center !important;
  margin-bottom: 30rpx;
}

.root {
  height: 100%;
  width: 100%;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
  /* min-height: 1000px; */
}

.nowTemp-light {
  color: rgba(201, 215, 230, 0.8);
  font-weight: 200;
  font-family: 'Lato', sans-serif;
}

.nowTemp-dark {
  color: rgb(112, 147, 191);
  font-weight: 200;
  font-family: 'Lato', sans-serif;
}

