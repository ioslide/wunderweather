.polute-line {
  background: #fff;
  position: absolute;
  width: 8px;
  height: 8px;
  border-radius: 50%;
}

@-webkit-keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

@keyframes fadeIn {
  from {
    opacity: 0;
  }

  to {
    opacity: 1;
  }
}

@-webkit-keyframes fadeOut {
  from {
    opacity: 1;
  }

  to {
    opacity: 0;
  }
}

@keyframes fadeOut {
  from {
    opacity: 1;
  }

  to {
    opacity: 0;
  }
}


.capsuleFadeIn {
  opacity: 1;
  animation: fadeIn 1s ease-in;
}

.capsuleFadeOut {
  opacity: 0;
  animation: fadeOut 1s ease;
}

